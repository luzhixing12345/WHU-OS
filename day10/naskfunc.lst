   147 000000C0                                 _memtest_sub:	; unsigned int memtest_sub(unsigned int start, unsigned int end)
   148 000000C0 57                              		PUSH	EDI						; （EBX, ESI, EDI も使いたいので）
   149 000000C1 56                              		PUSH	ESI
   150 000000C2 53                              		PUSH	EBX
   151 000000C3 BE AA55AA55                     		MOV		ESI,0xaa55aa55			; pat0 = 0xaa55aa55;
   152 000000C8 BF 55AA55AA                     		MOV		EDI,0x55aa55aa			; pat1 = 0x55aa55aa;
   153 000000CD 8B 44 24 10                     		MOV		EAX,[ESP+12+4]			; i = start;
   154 000000D1                                 mts_loop:
   155 000000D1 89 C3                           		MOV		EBX,EAX
   156 000000D3 81 C3 00000FFC                  		ADD		EBX,0xffc				; p = i + 0xffc;
   157 000000D9 8B 13                           		MOV		EDX,[EBX]				; old = *p;
   158 000000DB 89 33                           		MOV		[EBX],ESI				; *p = pat0;
   159 000000DD 83 33 FF                        		XOR		DWORD [EBX],0xffffffff	; *p ^= 0xffffffff;
   160 000000E0 3B 3B                           		CMP		EDI,[EBX]				; if (*p != pat1) goto fin;
   161 000000E2 75 18                           		JNE		mts_fin
   162 000000E4 83 33 FF                        		XOR		DWORD [EBX],0xffffffff	; *p ^= 0xffffffff;
   163 000000E7 3B 33                           		CMP		ESI,[EBX]				; if (*p != pat0) goto fin;
   164 000000E9 75 11                           		JNE		mts_fin
   165 000000EB 89 13                           		MOV		[EBX],EDX				; *p = old;
   166 000000ED 05 00001000                     		ADD		EAX,0x1000				; i += 0x1000;
   167 000000F2 3B 44 24 14                     		CMP		EAX,[ESP+12+8]			; if (i <= end) goto mts_loop;
   168 000000F6 76 D9                           		JBE		mts_loop
   169 000000F8 5B                              		POP		EBX
   170 000000F9 5E                              		POP		ESI
   171 000000FA 5F                              		POP		EDI
   172 000000FB C3                              		RET
